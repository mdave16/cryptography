{
  "name": "cryptography",
  "homepage": "http://mdave16.github.io/cryptography",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "rmcache": "rm -rf node_modules/gh-pages/.cache",
    "predeploy": "yarn rmcache & yarn build",
    "deploy": "gh-pages -d build",
    "serve": "yarn build & serve -s build -l 3000",
    "format": "yarn prettier --no-config --write --use-tabs --no-semi  \"**/*.js\"",
    "upgrade-deps": "yarn upgrade --force -L --emoji"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^2.2.0",
    "prettier": "1.19.1"
  },
  "engines": {
    "node": "14.15.1"
  }
}
